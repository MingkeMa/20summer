{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "-Wall",
                "r_lunar.cpp",
                "-IC:/lua-5.3.4/install/include/",
                "-LC:/lua-5.3.4/install/lib/",
                "-o",
                "a",
                "-llua",
                "-ldl"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}